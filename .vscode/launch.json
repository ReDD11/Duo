  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "args": ["-f 16O2_fitting_001.inp", "16O2_fitting.out"],
      "console": "integratedTerminal",
      "justMyCode": true
    },
    {
      "name": "(gdb) Fortran + Build",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/j-duo-v218v1.x",
      "args": [
        "<",
        "16O2_abinitio_magdipole.inp",
        ">",
        "16O2_abinitio_magdipole.out"
      ], // Possible input args for a.out
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "build",
    },
    {
      "name": "(gdb) Fortran",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/j-duo-v218v1.x",
      "args": [
        "<",
        "16O2_abinitio_magdipole.inp",
        ">",
        "16O2_abinitio_magdipole.out"
      ], // Possible input args for a.out
      "stopAtEntry": true,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}