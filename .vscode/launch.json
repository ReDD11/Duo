    "version": "0.2.0",
    "configurations": [
      {
        "name": "(gdb) Fortran + Build",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/duo.x",
        "args": [
          "<",
          "16O2_4states_MD.inp",
          ">",
          "16O2_4states_MD.out"
        ], // Possible input args for a.out
        "stopAtEntry": false,
        "cwd": "${workspaceFolder}",
        "environment": [],
        "externalConsole": false,
        "MIMode": "gdb",
        "setupCommands": [
          {
            "description": "Enable pretty-printing for gdb",
            "text": "-enable-pretty-printing",
            "ignoreFailures": true
          }
        ],
        "preLaunchTask": "build",
      },
      {
        "name": "(gdb) Fortran",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/duo.x",
        "args": [
          "<",
          "16O2_4states_MD.inp",
          ">",
          "16O2_4states_MD.out"
        ], // Possible input args for a.out
        "stopAtEntry": false,
        "cwd": "${workspaceFolder}",
        "environment": [],
        "externalConsole": false,
        "MIMode": "gdb",
        "setupCommands": [
          {
            "description": "Enable pretty-printing for gdb",
            "text": "-enable-pretty-printing",
            "ignoreFailures": true
          }
        ]
      }
    ]
  }